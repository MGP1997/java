 */
public class ForTest {
	
	public static void main(String[] args) {
		
		//for 기본 조기식; 조건식; 증감식
		for(int i = 0; i < 10; i++) {
			System.out.println("i : " + i);
		}
		//1부터 10까지 합
		
		//1부터 10까지 짝수합
		
		//for 중첩
		
		//구구단
		
		//별삼각형
		
		
		
		
		
		
		
		
		
	}

}
