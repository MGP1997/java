/*
 *  날짜 : 2025/07/08
 *  이름 : 박민규
 *  내용 : 배열(Array) 실습하기
 */
public class ArrayTest {
	public static void main(String[] args) {
		
		//배열
		int[] arr1 = {1, 2, 3, 4, 5};
		char[] arr2 = {'A', 'B', 'C'};
		string[] arr3 = {"서울", "대전", "대구", "부산", "광주"};	
				
		//배열 원소
		System.out.println("arr1[0] : " + arr1[0]);
		System.out.println("arr2[1] : " + arr2[1]);
		System.out.println("arr3[2] : " + arr3[2]);
		//배열 길이
		
		//배열 반복문
		
		//1차원 배열
		
		//2차원 배열
		
		//3차원 배열
				
		
		
		
		
		
	}
}
