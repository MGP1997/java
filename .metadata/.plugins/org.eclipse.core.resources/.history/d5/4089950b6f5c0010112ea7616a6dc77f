package test1;
/*
 * 날짜 : 2025/07/09
 * 이름 : 박민규
 * 내용 : 자바 반복문 연습 문제
 */
public class test8 {
	public static void main(String[] args) {
		
		int n = 5;
		
		for(int i = 0; i < n ; i++) {
			
			for(int j = n - 1; j < n; j--) {
				System.out.print(" ");
				
				for(int j = 0; j < n; j++) {
					System.out.print("*");
				}
				System.out.print("\n");
			}
		}
	}

}
