package sub2;

/**
 *  날짜 : 2025/07/07
 *  이름 : 박민규
 *  내용 : Java 다양한 출력 실습
 *  
 */
public class PrintTest {

	public static void main(String[] args) {
		
		// 기본 출력
		System.out.println("Hello Java!");  // 한줄복사 컨트롤 알트 아래
		System.out.println("Hello Kroea!"); // 한줄삭제 컨트롤 d
		System.out.println("Hello Busan!"); // 작업 취소 컨트롤 z

		
		// 한줄 출력
		System.out.print("Welcome"); // ln 줄바꿈
		System.out.print("Java!");
		
		// 개행 출력
		//System.out.println();
		System.out.print("Welcome\n"); // \n : 개별처리 이스케이프 특수문자
		System.out.print("Korea");
		
	}
}
